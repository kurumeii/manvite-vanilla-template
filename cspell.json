{
  "version": "0.2",
  "ignorePaths": [
    "**/node_modules/**",
    "**/dist/**",
    "**/*.lock",
    "**/package-lock.json"
  ],
  "dictionaryDefinitions": [
    {
      "name": "library",
      "path": "./src/assets/dictionary/lib.txt",
      "description": "A list of library related words"
    },
    {
      "name": "misc",
      "path": "./src/assets/dictionary/misc.txt",
      "description": "A list of project related words"
    }
  ],
  "dictionaries": ["library", "misc"],
  "cache": {
    "cacheStrategy": "metadata",
    "cacheFormat": "universal",
    "useCache": true
  },
  "language": "en, vi",
  "import": [
    "@cspell/dict-css/cspell-ext.json",
    "@cspell/dict-html/cspell-ext.json",
    "@cspell/dict-typescript/cspell-ext.json",
    "@cspell/dict-vi-vn/cspell-ext.json"
  ]
}
